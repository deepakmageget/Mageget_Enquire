<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
       <tab id="mageget" translate="label" sortOrder="250">
            <label>mageget</label>
        </tab>
        <section id="enquire" translate="label" sortOrder="150" showInDefault="1" showInWebsite="1" showInStore="1">
            <class>mageget-separator</class>
            <label>User Enquire Form</label>
            <tab>mageget</tab>
            <resource>Mageget_Enquire::configuration</resource>
            <group id="general" translate="label" type="text" sortOrder="10" showInDefault="1" showInWebsite="0" showInStore="0">
                <label>Configuration</label>
                <field id="enable" translate="label" type="select" sortOrder="1" showInDefault="1" showInWebsite="0" showInStore="0">
                    <label>Enable Module</label>
                    <source_model>Magento\Config\Model\Config\Source\Enabledisable</source_model>
                </field>
            </group>

        </section>  
      
          <section id="slider" translate="label" sortOrder="160" showInDefault="1" showInWebsite="1" showInStore="1">
            <class>mageget-separator</class>
            <label>Banner Slider</label>
            <tab>mageget</tab>
            <resource>Mageget_Slider::configuration</resource>
            <group id="general" translate="label" type="text" sortOrder="10" showInDefault="1" showInWebsite="0" showInStore="0">
                <label>Configuration</label>
                <field id="enable" translate="label" type="select" sortOrder="1" showInDefault="1" showInWebsite="0" showInStore="0">
                    <label>Enable Module</label>
                    <source_model>Magento\Config\Model\Config\Source\Enabledisable</source_model>
                </field>
            </group>
        </section>

    </system>
</config>